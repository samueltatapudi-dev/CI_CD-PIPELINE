# Copy to .env and set values before running docker-compose

JENKINS_ADMIN_ID=admin
JENKINS_ADMIN_PASSWORD=admin123

# Docker Hub (or compatible registry) credentials
DOCKERHUB_USERNAME=your-dockerhub-username
DOCKERHUB_TOKEN=your-dockerhub-pat-or-password

# SonarQube: create a token in SonarQube UI -> My Account -> Security
SONARQUBE_TOKEN=your-sonarqube-token

# Docker image repo (namespace/repo)
IMAGE_REPO=your-dockerhub-namespace/ci-cd-app

# Kubernetes namespace to deploy
KUBE_NAMESPACE=default

# Repository to poll (override if needed)
APP_REPO_URL=https://github.com/samueltatapudi-dev/CI_CD_APP_VERSION
APP_REPO_BRANCH=main
